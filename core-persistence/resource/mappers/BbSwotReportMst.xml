<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="BbSwotReportMst">

	<resultMap type="com.netsteadfast.greenstep.po.hbm.BbSwotReportMst" id="baseResultMap">
		<id column="OID" property="oid"/>				
		<result column="REPORT_ID" property="reportId"/>
		<result column="VISION_TITLE" property="visionTitle"/>
		<result column="ORG_NAME" property="orgName"/>				
		<result column="CUSERID" property="cuserid"/>
		<result column="CDATE" property="cdate"/>
		<result column="UUSERID" property="uuserid"/>		
		<result column="UDATE" property="udate"/>		
	</resultMap>
	
	<select id="selectByParams" resultMap="baseResultMap" >
		select * from bb_swot_report_mst where 1=1
		<if test="oid != null">
			AND OID = #{oid}
		</if>			
		<if test="reportId != null">
			AND REPORT_ID = #{reportId}
		</if>										
	</select>
	
	<select id="selectByValue" resultMap="baseResultMap" >
		select * from bb_swot_report_mst where 1=1
		<if test="oid != null">
			AND OID = #{oid}
		</if>			
		<if test="reportId != null">
			AND REPORT_ID = #{reportId}
		</if>						
	</select>	
	
</mapper>