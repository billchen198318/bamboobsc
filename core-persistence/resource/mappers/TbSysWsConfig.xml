<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="TbSysWsConfig">

	<resultMap type="com.netsteadfast.greenstep.po.hbm.TbSysWsConfig" id="baseResultMap">
		<id column="OID" property="oid"/>						
		<result column="WS_ID" property="wsId"/>
		<result column="SYSTEM" property="system"/>								
		<result column="TYPE" property="type"/>		
		<result column="BEAN_ID" property="beanId"/>		
		<result column="PUBLISH_ADDRESS" property="publishAddress"/>		
		<result column="DESCRIPTION" property="description"/>				
		<result column="CUSERID" property="cuserid"/>
		<result column="CDATE" property="cdate"/>
		<result column="UUSERID" property="uuserid"/>		
		<result column="UDATE" property="udate"/>		
	</resultMap>
	
	<select id="selectByParams" resultMap="baseResultMap" >
		select * from tb_sys_ws_config where 1=1
		<if test="oid != null">
			AND OID = #{oid}
		</if>	
		<if test="wsId != null">
			AND WS_ID = #{wsId}
		</if>	
		<if test="system != null">
			AND SYSTEM = #{system}
		</if>					
		<if test="type != null">
			AND TYPE = #{type}
		</if>																
	</select>
	
	<select id="selectByValue" resultMap="baseResultMap" >
		select * from tb_sys_ws_config where 1=1
		<if test="oid != null">
			AND OID = #{oid}
		</if>	
		<if test="wsId != null">
			AND WS_ID = #{wsId}
		</if>			
		<if test="system != null">
			AND SYSTEM = #{system}
		</if>		
		<if test="type != null">
			AND TYPE = #{type}
		</if>				
	</select>	
	
</mapper>