<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="TbSysMenu">

	<resultMap type="com.netsteadfast.greenstep.po.hbm.TbSysMenu" id="baseResultMap">
		<id column="OID" property="oid"/>
		<result column="PROG_ID" property="progId"/>				
		<result column="PARENT_OID" property="parentOid"/>						
		<result column="ENABLE_FLAG" property="enableFlag"/>						
		<result column="CUSERID" property="cuserid"/>
		<result column="CDATE" property="cdate"/>
		<result column="UUSERID" property="uuserid"/>		
		<result column="UDATE" property="udate"/>		
	</resultMap>
	
	<select id="selectByParams" resultMap="baseResultMap" >
		select * from tb_sys_menu where 1=1
		<if test="oid != null">
			AND OID = #{oid}
		</if>		
		<if test="progId != null">
			AND PROG_ID = #{progId}
		</if>		
		<if test="parentOid != null">
			AND PARENT_OID = #{parentOid}
		</if>										
	</select>
	
	<select id="selectByValue" resultMap="baseResultMap" >
		select * from tb_sys_menu where 1=1
		<if test="oid != null">
			AND OID = #{oid}
		</if>		
		<if test="progId != null">
			AND PROG_ID = #{progId}
		</if>		
		<if test="parentOid != null">
			AND PARENT_OID = #{parentOid}
		</if>					
	</select>	
	
</mapper>